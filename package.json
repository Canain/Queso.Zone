{
  "name": "queso",
  "version": "1.0.0",
  "description": "A code tutorial replay tool with compilation and audio",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Canain/queso.git"
  },
  "author": "Shuyang Chen <sc1085@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Canain/queso/issues"
  },
  "homepage": "https://github.com/Canain/queso#readme",
  "scripts": {
    "serve": "live-server --entry-file=index.html public",
    "watch:browser:debug:webpack": "webpack -w --config config/webpack.config.js",
    "watch:browser:debug:html": "watch \"npm run build:browser:debug:html\" src/browser/html",
    "watch:browser:debug": "concurrently -rk \"npm run watch:browser:debug:webpack\" \"npm run watch:browser:debug:html\"",
    "watch:browser:prod:webpack": "webpack -wp --config config/webpack.prod.js",
    "watch:browser:prod:html": "watch \"npm run build:browser:prod:html\" src/browser/html",
    "watch:browser:prod": "concurrently -rk \"npm run watch:browser:prod:webpack\" \"npm run watch:browser:prod:html\"",
    "watch:server": "tsc -w -p src/server",
    "build:browser:debug:html": "html-minifier --input-dir src/browser/html --output-dir public",
    "build:browser:debug:webpack": "webpack --config config/webpack.config.js",
    "build:browser:debug": "concurrently -r \"npm run build:browser:debug:html\" \"npm run build:browser:debug:webpack\"",
    "build:browser:prod:html": "html-minifier -c config/htmlmin.json --input-dir src/browser/html --output-dir public",
    "build:browser:prod:webpack": "webpack -p --config config/webpack.prod.js",
    "build:browser:prod": "concurrently -r \"npm run build:browser:prod:html\" \"npm run build:browser:prod:webpack\"",
    "build:server": "tsc -p src/server",
    "start:browser:debug": "npm run build:browser:debug && concurrently -rk \"npm run watch:browser:debug\" \"npm run serve\"",
    "start:browser:prod": "npm run build:browser:prod && concurrently -rk \"npm run watch:browser:prod\" \"npm run serve\"",
    "nodemon": "nodemon --watch dist --watch .env index.js",
    "start:server:debug": "npm run build:server && concurrently -rk \"npm run nodemon\" \"npm run watch:server\"",
    "start:server:prod": "npm run build:server && node .",
    "start:server": "npm run start:server:debug",
    "start": "npm run start:browser:debug",
    "start:debug": "concurrently -rk \"npm run start:browser:debug\" \"npm run start:server\"",
    "start:prod": "concurrently -rk \"npm run start:browser:prod\" \"npm run start:server\"",
    "release": "npm run build:browser:prod && firebase deploy",
    "release:debug": "npm run release && npm run start:debug"
  },
  "dependencies": {
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.12",
    "@types/react-dom": "^0.14.23",
    "@types/react-ga": "^1.4.7",
    "@types/react-router": "^3.0.5",
    "@types/source-map-support": "^0.2.28",
    "awesome-typescript-loader": "^3.0.7",
    "babel-core": "^6.23.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "concurrently": "^3.3.0",
    "css-loader": "^0.26.2",
    "dotenv": "^4.0.0",
    "firebase": "^3.6.10",
    "html-minifier": "^3.3.3",
    "json-loader": "^0.5.4",
    "live-server": "^1.2.0",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.3.2",
    "precss": "^1.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-router": "^3.0.2",
    "regenerator-runtime": "^0.10.3",
    "shallowequal": "^0.2.2",
    "source-map-support": "^0.4.11",
    "style-loader": "^0.13.2",
    "typescript": "^2.2.1",
    "watch": "^1.0.2",
    "webpack": "^2.2.1"
  }
}
